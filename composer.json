{
  "name": "taxibeat/laravel-prometheus-exporter",
  "description": "A prometheus exporter for Laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Taxibeat"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "illuminate/support": "^5.3",
    "illuminate/routing": "^5.3",
    "jimdo/prometheus_client_php": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/taxibeat/prometheus_client_php"
    }
  ],
  "autoload": {
    "psr-4": {
      "Taxibeat\\LaravelPrometheusExporter\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5",
    "mockery/mockery": "^0.9.5",
    "friendsofphp/php-cs-fixer": "2.12.1"
  }
}
