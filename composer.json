{
  "name": "thebeatapp/pyr",
  "description": "A Prometheus bridge for Lumen",
  "license": "MIT",
  "authors": [
    {
      "name": "Taxibeat"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "illuminate/support": "^5.3",
    "illuminate/routing": "^5.3",
    "thebeatapp/prometheus_client_php": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/thebeatapp/prometheus_client_php"
    }
  ],
  "autoload": {
    "psr-4": {
      "Taxibeat\\Pyr\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^0.9.5",
    "friendsofphp/php-cs-fixer": "2.*",
    "orchestra/testbench": "^3.5"
  }
}
